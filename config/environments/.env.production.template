# Production Environment Variables Template

# Database
POSTGRES_PASSWORD=your_secure_postgres_password
POSTGRES_REPLICATION_PASSWORD=your_replication_password

# Message Queue
RABBITMQ_PASSWORD=your_rabbitmq_password

# Kafka
KAFKA_BOOTSTRAP_SERVERS=kafka:9092

# Vault
VAULT_URL=http://vault:8200
VAULT_TOKEN=your_vault_token

# MLflow
MLFLOW_TRACKING_URI=http://mlflow:5000

# External Services
PINECONE_API_KEY=your_pinecone_api_key
PINECONE_ENVIRONMENT=us-west1-gcp
PINECONE_INDEX=auterity-vectors

WEAVIATE_API_KEY=your_weaviate_api_key
WEAVIATE_URL=http://weaviate:8080

# LLM Providers
OPENAI_API_KEY=your_openai_api_key
OPENAI_ORG_ID=your_openai_org_id
ANTHROPIC_API_KEY=your_anthropic_api_key

# Authentication
AUTH0_DOMAIN=your_auth0_domain
AUTH0_CLIENT_ID=your_auth0_client_id
AUTH0_CLIENT_SECRET=your_auth0_client_secret

# AWS (if using Cognito)
AWS_REGION=us-east-1
COGNITO_USER_POOL_ID=your_cognito_pool_id
COGNITO_CLIENT_ID=your_cognito_client_id

# Monitoring
GRAFANA_PASSWORD=your_grafana_password
SENTRY_DSN=your_sentry_dsn

# Security
JWT_SECRET_KEY=your_jwt_secret_key_minimum_32_characters
ENCRYPTION_KEY=your_encryption_key_32_characters

# SSL Certificates (paths)
SSL_CERT_PATH=/etc/ssl/certs/auterity.crt
SSL_KEY_PATH=/etc/ssl/certs/auterity.key
