{
  "name": "@auterity/workflow-studio",
  "version": "1.0.0",
  "description": "Auterity Workflow Studio - Unified Visual Workflow Builder",
  "private": true,
  "type": "module",
  "main": "src/index.ts",
  "module": "src/index.ts",
  "types": "src/index.ts",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:analyze": "npm run build && npx vite-bundle-analyzer dist/stats.html",
    "preview": "vite preview",
    "test": "vitest run --config vitest.config.ts",
    "test:watch": "vitest --config vitest.config.ts",
    "test:coverage": "vitest run --coverage --config vitest.config.ts",
    "test:ui": "vitest --ui",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist node_modules/.vite",
    "prebuild": "npm run type-check && npm run lint",
    "deploy:staging": "npm run build && vercel --prod=false",
    "deploy:production": "npm run build && vercel --prod=true"
  },
  "dependencies": {
    "@auterity/design-system": "workspace:*",
    "@auterity/workflow-contracts": "workspace:*",
    "@xyflow/react": "^12.8.4",
    "@ai-sdk/anthropic": "^2.0.9",
    "@ai-sdk/azure": "^2.0.23",
    "@ai-sdk/cohere": "^2.0.7",
    "@ai-sdk/google": "^2.0.11",
    "@ai-sdk/openai": "^2.0.23",
    "@ai-sdk/react": "^2.0.28",
    "ai": "^5.0.28",
    "framer-motion": "^12.23.12",
    "immer": "^10.1.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.18.0",
    "recharts": "^3.1.2",
    "socket.io-client": "^4.8.1",
    "zod": "^4.1.5",
    "date-fns": "^4.1.0",
    "clsx": "^2.1.1",
    "tailwind-merge": "^2.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/node": "^24.3.0",
    "@vitejs/plugin-react": "^4.1.1",
    "@vitest/ui": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@testing-library/react": "^13.4.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/user-event": "^14.5.1",
    "jsdom": "^22.1.0",
    "vite": "^7.1.3",
    "typescript": "^5.2.2",
    "vitest": "^3.2.4",
    "eslint": "^8.57.1",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.0",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.5",
    "rollup-plugin-visualizer": "^6.0.3",
    "vite-bundle-analyzer": "^1.1.0"
  },
  "peerDependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
