
// .vo file for v0.dev or compatible JSX visual builder

<vstack gap="4">
  <h2>Error Analytics Dashboard</h2>
  <card>
    <h3>Top Error Types</h3>
    <bar-chart data={errorTypeData} />
  </card>
  <card>
    <h3>Workflows With Most Failures</h3>
    <table rows={errorWorkflowRows} />
  </card>
  <card>
    <h3>Resolution Time (avg)</h3>
    <line-chart data={resolutionTimeData} />
  </card>
</vstack>
