# Integration Service Configuration
NODE_ENV=development
INTEGRATION_PORT=3002

# Security
JWT_SECRET=your-jwt-secret-key-here
JWT_REFRESH_SECRET=your-jwt-refresh-secret-key-here

# Database & Cache
REDIS_URL=redis://localhost:6379
RABBITMQ_URL=amqp://localhost:5672

# System Credentials
AUTERITY_USERNAME=auterity_system
AUTERITY_PASSWORD=secure_password
AUTERITY_API_KEY=your_auterity_api_key

RELAYCORE_USERNAME=relaycore_system
RELAYCORE_PASSWORD=secure_password
RELAYCORE_API_KEY=your_relaycore_api_key

NEUROWEAVER_USERNAME=neuroweaver_system
NEUROWEAVER_PASSWORD=secure_password
NEUROWEAVER_API_KEY=your_neuroweaver_api_key

# CORS Configuration
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:3001,http://localhost:3003

# Health Monitoring
HEALTH_CHECK_INTERVAL=30000
ENABLE_ALERTS=true
SHUTDOWN_TIMEOUT=10000
