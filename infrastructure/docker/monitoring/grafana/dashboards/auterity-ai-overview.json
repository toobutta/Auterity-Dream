{
  "dashboard": {
    "id": null,
    "title": "Auterity AI Platform Overview",
    "tags": ["auterity", "ai", "monitoring"],
    "timezone": "UTC",
    "panels": [
      {
        "id": 1,
        "title": "Service Health Status",
        "type": "stat",
        "targets": [
          {
            "expr": "up{job=~\"auterity-.*\"}",
            "legendFormat": "{{job}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {
                "options": {
                  "0": {
                    "text": "DOWN",
                    "color": "red"
                  },
                  "1": {
                    "text": "UP",
                    "color": "green"
                  }
                },
                "type": "value"
              }
            ]
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "AI Service Response Time",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket{job=~\"auterity-(vllm|langgraph|crewai)\"}[5m]))",
            "legendFormat": "{{job}} P95",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.50, rate(http_request_duration_seconds_bucket{job=~\"auterity-(vllm|langgraph|crewai)\"}[5m]))",
            "legendFormat": "{{job}} P50",
            "refId": "B"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "Request Rate by Service",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(http_requests_total{job=~\"auterity-.*\"}[5m])",
            "legendFormat": "{{job}}",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 8
        }
      },
      {
        "id": 4,
        "title": "Error Rate by Service",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(http_requests_total{job=~\"auterity-.*\", status=~\"5..\"}[5m]) / rate(http_requests_total{job=~\"auterity-.*\"}[5m]) * 100",
            "legendFormat": "{{job}} error rate",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 8
        }
      },
      {
        "id": 5,
        "title": "AI Model Performance",
        "type": "table",
        "targets": [
          {
            "expr": "ai_model_inference_duration{job=\"auterity-vllm\"}",
            "legendFormat": "{{model}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "align": "left",
              "displayMode": "auto"
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 16
        }
      },
      {
        "id": 6,
        "title": "Workflow Execution Metrics",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(workflow_executions_total{job=\"auterity-langgraph\"}[5m])",
            "legendFormat": "Workflow executions",
            "refId": "A"
          },
          {
            "expr": "rate(workflow_executions_total{job=\"auterity-langgraph\", status=\"completed\"}[5m])",
            "legendFormat": "Completed workflows",
            "refId": "B"
          },
          {
            "expr": "rate(workflow_executions_total{job=\"auterity-langgraph\", status=\"failed\"}[5m])",
            "legendFormat": "Failed workflows",
            "refId": "C"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 16
        }
      },
      {
        "id": 7,
        "title": "Agent Performance Metrics",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(agent_executions_total{job=\"auterity-crewai\"}[5m])",
            "legendFormat": "Agent executions",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(agent_execution_duration_bucket{job=\"auterity-crewai\"}[5m]))",
            "legendFormat": "Agent execution P95",
            "refId": "B"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 24
        }
      },
      {
        "id": 8,
        "title": "Resource Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(container_cpu_usage_seconds_total{container=~\"auterity-.*\"}[5m])",
            "legendFormat": "{{container}} CPU",
            "refId": "A"
          },
          {
            "expr": "container_memory_usage_bytes{container=~\"auterity-.*\"}",
            "legendFormat": "{{container}} Memory",
            "refId": "B"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 24
        }
      },
      {
        "id": 9,
        "title": "Active Alerts",
        "type": "table",
        "targets": [
          {
            "expr": "ALERTS{alertstate=\"firing\"}",
            "legendFormat": "{{alertname}}",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 32
        }
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {},
    "templating": {
      "list": []
    },
    "annotations": {
      "list": []
    },
    "refresh": "30s",
    "schemaVersion": 27,
    "version": 0,
    "links": []
  }
}

