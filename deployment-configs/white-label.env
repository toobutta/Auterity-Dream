# White-label Deployment Configuration
# Customizable configuration for client-specific white-label deployments

# ==================================================
# DEPLOYMENT TYPE & ENVIRONMENT
# ==================================================
VITE_DEPLOYMENT_TYPE=white-label
NODE_ENV=production
VITE_ENVIRONMENT=production

# ==================================================
# CLIENT-SPECIFIC BRANDING
# ==================================================
VITE_CLIENT_NAME=ClientCorp
VITE_CLIENT_DOMAIN=workflow.clientcorp.com
VITE_CLIENT_LOGO_URL=https://cdn.clientcorp.com/logo.png
VITE_CLIENT_FAVICON_URL=https://cdn.clientcorp.com/favicon.ico
VITE_CLIENT_SUPPORT_EMAIL=support@clientcorp.com

# ==================================================
# BRANDING THEME CONFIGURATION
# ==================================================
VITE_THEME_PRIMARY_COLOR=#1E40AF
VITE_THEME_SECONDARY_COLOR=#64748B
VITE_THEME_ACCENT_COLOR=#F59E0B
VITE_THEME_SUCCESS_COLOR=#10B981
VITE_THEME_WARNING_COLOR=#F59E0B
VITE_THEME_ERROR_COLOR=#EF4444

# ==================================================
# CUSTOM NAVIGATION & MENUS
# ==================================================
VITE_NAVIGATION_HIDE_AUTERITY_BRANDING=true
VITE_NAVIGATION_CUSTOM_ITEMS=["Dashboard","Workflows","Analytics","Reports"]
VITE_NAVIGATION_HIDE_ENTERPRISE_TAB=false

# ==================================================
# API & BACKEND CONFIGURATION
# ==================================================
VITE_API_BASE_URL=https://api.clientcorp.auterity.com/v1
VITE_WS_BASE_URL=wss://api.clientcorp.auterity.com
VITE_CDN_BASE_URL=https://cdn.clientcorp.auterity.com

# ==================================================
# AUTHENTICATION & SECURITY
# ==================================================
VITE_AUTH_PROVIDER=client-sso
VITE_AUTH_SSO_URL=https://sso.clientcorp.com/oauth
VITE_AUTH_CLIENT_ID=clientcorp_workflow_app
VITE_AUTH_REDIRECT_URI=https://workflow.clientcorp.com/callback

# ==================================================
# AI SERVICE PROVIDERS
# ==================================================
VITE_OPENAI_API_KEY=client_openai_key
VITE_ANTHROPIC_API_KEY=client_anthropic_key
VITE_GOOGLE_API_KEY=client_google_key
VITE_HUGGINGFACE_API_KEY=client_huggingface_key
VITE_NOVITA_API_KEY=client_novita_key

# ==================================================
# CLIENT-SPECIFIC FEATURES
# ==================================================
VITE_ENABLE_CLIENT_INTEGRATIONS=true
VITE_CLIENT_INTEGRATIONS=["salesforce","servicenow","jira"]
VITE_ENABLE_CLIENT_BRANDING=true
VITE_ENABLE_CLIENT_SUPPORT_PORTAL=true

# ==================================================
# LICENSE & SUBSCRIPTION
# ==================================================
VITE_LICENSE_KEY=XXXX-XXXX-XXXX-XXXX
VITE_SUBSCRIPTION_PLAN=white-label-enterprise
VITE_BILLING_MANAGED_BY_CLIENT=false

# ==================================================
# CUSTOM DASHBOARDS & REPORTS
# ==================================================
VITE_ENABLE_CUSTOM_DASHBOARDS=true
VITE_CUSTOM_DASHBOARD_CONFIG_URL=https://cdn.clientcorp.com/dashboard-config.json
VITE_ENABLE_CLIENT_REPORTS=true
VITE_CLIENT_REPORT_TEMPLATES=["compliance","usage","performance"]

# ==================================================
# INTEGRATION ENDPOINTS
# ==================================================
VITE_CLIENT_API_ENDPOINTS={
  "salesforce": "https://clientcorp.my.salesforce.com",
  "servicenow": "https://clientcorp.servicenow.com",
  "jira": "https://clientcorp.atlassian.net"
}

# ==================================================
# SECURITY & COMPLIANCE
# ==================================================
VITE_SECURITY_LEVEL=enterprise
VITE_COMPLIANCE_FRAMEWORK=HIPAA
VITE_DATA_ENCRYPTION_LEVEL=aes256
VITE_AUDIT_LOG_RETENTION=3650

# ==================================================
# PERFORMANCE & MONITORING
# ==================================================
VITE_PERFORMANCE_MONITORING_ENABLED=true
VITE_ERROR_TRACKING_ENABLED=true
VITE_USAGE_ANALYTICS_ENABLED=true
VITE_CLIENT_METRICS_ENABLED=true

# ==================================================
# SUPPORT & DOCUMENTATION
# ==================================================
VITE_SUPPORT_PORTAL_URL=https://support.clientcorp.com/workflow
VITE_DOCUMENTATION_URL=https://docs.clientcorp.com/workflow
VITE_TRAINING_MATERIALS_URL=https://training.clientcorp.com

# ==================================================
# WHITE-LABEL SPECIFIC FEATURES
# ==================================================
VITE_HIDE_AUTERITY_REFERENCES=true
VITE_CUSTOM_LOGIN_PAGE=true
VITE_CUSTOM_ERROR_PAGES=true
VITE_CUSTOM_LOADING_SCREENS=true
VITE_WHITE_LABEL_SUPPORT_CHAT=true

# ==================================================
# CLIENT DATA MANAGEMENT
# ==================================================
VITE_DATA_ISOLATION_LEVEL=tenant
VITE_CLIENT_DATA_RETENTION=2555
VITE_BACKUP_FREQUENCY=daily
VITE_CLIENT_DATA_EXPORT=true

# ==================================================
# ADVANCED CONFIGURATIONS
# ==================================================
VITE_CUSTOM_WORKFLOW_TEMPLATES=true
VITE_CLIENT_APPROVED_MODELS=["gpt-4","claude-3","gemini-pro"]
VITE_MAX_USERS_PER_CLIENT=10000
VITE_API_RATE_LIMIT=50000
VITE_STORAGE_QUOTA=107374182400
