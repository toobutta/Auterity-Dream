# SaaS Enterprise Deployment Configuration
# Production-ready configuration for Auterity Error-IQ SaaS

# ==================================================
# DEPLOYMENT TYPE & ENVIRONMENT
# ==================================================
VITE_DEPLOYMENT_TYPE=saas
NODE_ENV=production
VITE_ENVIRONMENT=production

# ==================================================
# API & BACKEND CONFIGURATION
# ==================================================
VITE_API_BASE_URL=https://api.auterity.com/v1
VITE_WS_BASE_URL=wss://api.auterity.com
VITE_CDN_BASE_URL=https://cdn.auterity.com

# ==================================================
# AUTHENTICATION & SECURITY
# ==================================================
VITE_AUTH_PROVIDER=auth0
VITE_AUTH_DOMAIN=auterity.auth0.com
VITE_AUTH_CLIENT_ID=your_auth0_client_id
VITE_AUTH_AUDIENCE=https://api.auterity.com
VITE_AUTH_REDIRECT_URI=https://app.auterity.com/callback

# ==================================================
# AI SERVICE PROVIDERS
# ==================================================
VITE_OPENAI_API_KEY=your_openai_key
VITE_ANTHROPIC_API_KEY=your_anthropic_key
VITE_GOOGLE_API_KEY=your_google_key
VITE_HUGGINGFACE_API_KEY=your_huggingface_key
VITE_NOVITA_API_KEY=your_novita_key

# ==================================================
# FEATURE FLAGS & SUBSCRIPTION
# ==================================================
VITE_ENABLE_MULTITENANT=true
VITE_ENABLE_ENTERPRISE_FEATURES=true
VITE_ENABLE_WHITE_LABEL=false
VITE_ENABLE_USAGE_TRACKING=true
VITE_ENABLE_BILLING_INTEGRATION=true

# ==================================================
# ANALYTICS & MONITORING
# ==================================================
VITE_ANALYTICS_PROVIDER=mixpanel
VITE_ANALYTICS_TOKEN=your_mixpanel_token
VITE_ERROR_TRACKING_PROVIDER=sentry
VITE_SENTRY_DSN=your_sentry_dsn
VITE_MONITORING_ENDPOINT=https://monitoring.auterity.com

# ==================================================
# PERFORMANCE & CACHING
# ==================================================
VITE_CACHE_STRATEGY=aggressive
VITE_CACHE_TTL=3600000
VITE_ENABLE_SERVICE_WORKER=true
VITE_ENABLE_BACKGROUND_SYNC=true

# ==================================================
# BILLING & SUBSCRIPTION
# ==================================================
VITE_STRIPE_PUBLISHABLE_KEY=your_stripe_publishable_key
VITE_BILLING_API_URL=https://billing.auterity.com
VITE_SUBSCRIPTION_WEBHOOK_SECRET=your_webhook_secret

# ==================================================
# CDN & STATIC ASSETS
# ==================================================
VITE_ASSETS_BASE_URL=https://cdn.auterity.com/assets
VITE_IMAGES_BASE_URL=https://cdn.auterity.com/images
VITE_DOCS_BASE_URL=https://docs.auterity.com

# ==================================================
# EXTERNAL INTEGRATIONS
# ==================================================
VITE_SLACK_CLIENT_ID=your_slack_client_id
VITE_GITHUB_CLIENT_ID=your_github_client_id
VITE_ZAPIER_WEBHOOK_URL=https://hooks.zapier.com/hooks/catch/your_webhook_id/

# ==================================================
# ENTERPRISE FEATURES
# ==================================================
VITE_ENABLE_AUDIT_LOGS=true
VITE_ENABLE_COMPLIANCE_REPORTING=true
VITE_ENABLE_DATA_EXPORT=true
VITE_ENABLE_USER_MANAGEMENT=true
VITE_ENABLE_ROLE_BASED_ACCESS=true

# ==================================================
# LIMITS & QUOTAS
# ==================================================
VITE_MAX_WORKFLOWS_PER_USER=1000
VITE_MAX_API_CALLS_PER_HOUR=10000
VITE_MAX_STORAGE_PER_USER=5368709120
VITE_MAX_CONCURRENT_WORKFLOWS=50

# ==================================================
# SUPPORT & HELP
# ==================================================
VITE_SUPPORT_EMAIL=enterprise@auterity.com
VITE_SUPPORT_CHAT_ENABLED=true
VITE_SUPPORT_PHONE=+1-800-AUTERITY
VITE_DOCUMENTATION_URL=https://docs.auterity.com/enterprise

# ==================================================
# LEGAL & COMPLIANCE
# ==================================================
VITE_PRIVACY_POLICY_URL=https://auterity.com/privacy
VITE_TERMS_OF_SERVICE_URL=https://auterity.com/terms
VITE_COMPLIANCE_FRAMEWORK=SOC2_Type_II
VITE_DATA_RETENTION_DAYS=2555
